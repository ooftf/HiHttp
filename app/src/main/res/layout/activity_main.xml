<?xml version="1.0" encoding="utf-8"?>
<com.ooftf.hihttp.view.HiResponseLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/responseLayout"
    tools:context="com.ooftf.hihttp.sample.MainActivity">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!" />
        <com.ooftf.hihttp.view.HiResponseButton
            android:text="commit"
            android:id="@+id/commit"
            android:layout_width="match_parent"
            android:layout_height="48dp" />
    </LinearLayout>
</com.ooftf.hihttp.view.HiResponseLayout>
